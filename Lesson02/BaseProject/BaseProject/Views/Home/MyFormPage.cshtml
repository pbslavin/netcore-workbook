@model BaseProject.Models.FormDataModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MyForm</title>
</head>
<body>
    <disable-autocomplete>
        <form asp-action="/" method="post">
            <div>
                <label asp-for="FormString">Enter a string:</label>
                <input asp-for="FormString" />
            </div>
            <div>
                <label asp-for="FormString2">Enter another string, with help this time</label>
                <input asp-for="FormString2" autocomplete="on" />
            </div>
            <button type="submit">Submit</button>
        </form>
    </disable-autocomplete>
    <p>Click <a asp-controller="Home" asp-action="index">here</a> to go back to the index page.</p>
</body>
</html>